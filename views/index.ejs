  <section class="landing">
    <div class="landing-contents">
      <div class="contents">
        <span>Book a room</span>
        <h1>For your holidays & relaxation</h1>
        <div class="booking">
          <div class="tabs">
            <div class="item tab-active">
              <p>Lodging</p>
            </div>
          </div>
          <form id="bookingForm" novalidate method="POST" autocomplete="off">
            <div class="booking-container">
              <div class="item">
                <p>Check-in</p>
                <div class="input-group date" id="checkIn">
                  <input type="text" required data-parsley-required name="checkInDate" id="checkInDate"
                    class="form-control">
                  <div class="input-group-addon">
                    <span class="glyphicon glyphicon-th"></span>
                  </div>
                </div>
              </div>
              <div class="item" id="checkOutDate">
                <p>Check-out</p>
                <div class="input-group date" id="checkOut">
                  <input type="text" id="checkOutDate" name="checkOutDate" class="form-control">
                  <div class="input-group-addon">
                    <span class="glyphicon glyphicon-th"></span>
                  </div>
                </div>
              </div>
              <div class="item">
                <p>Adults</p>
                <input type="number" required data-parsley-required data-parsley-type="number" id="adults"
                  class="form-control">
              </div>
              <div class="item">
                <p>Children</p>
                <input type="number" required data-parsley-required data-parsley-type="number" id="children"
                  class="form-control">
              </div>
              <div class="item">
                <button type="button" class="custom-btn" data-toggle="modal" data-target="#continueModal">
                  Make reservation
                </button>
              </div>
              <div class="modal fade" id="continueModal" tabindex="-1" role="dialog" aria-labelledby="continueModal"
                aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered" role="document">
                  <div class="modal-content">
                    <div class="modal-header">
                      <h5 class="modal-title">Personal Details</h5>
                      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                      </button>
                    </div>
                    <div class="modal-body">
                      <div class="form-group">
                        <label for="name">Name</label>
                        <input type="text" name="name" id="name" required data-parsley-required class="form-control"
                          placeholder="Enter your name" aria-describedby="helpId">
                        <small id="helpId" class="text-muted">Eg: John Doe</small>
                      </div>
                      <div class="form-group">
                        <label for="email">Email</label>
                        <input type="email" class="form-control" name="email" id="email" aria-describedby="emailHelpId"
                          placeholder="Enter your email address" required data-parsley-required
                          data-parsley-type="email">
                        <small id="emailHelpId" class="form-text text-muted">Eg: johndoe@gmail.com</small>
                      </div>
                      <div class="form-group">
                        <label for="phone">Phone</label>
                        <input type="tel" name="phone" id="phone" class="form-control"
                          placeholder="Enter your phone number" aria-describedby="helpId" required data-parsley-required
                          data-parsley-type="number">
                        <small id="helpId" class="text-muted">234XXXXXX</small>
                      </div>
                    </div>
                    <div class="modal-footer">
                      <button type="submit" id="submitBtn" class="custom-btn">Submit</button>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </section>

